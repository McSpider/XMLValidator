<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" >
	<xs:element name="map">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="name" type="xs:string" maxOccurs="1" minOccurs="1" />
				<xs:element name="version" type="xs:string" maxOccurs="1" minOccurs="1" />
				<xs:element name="objective" type="xs:string" maxOccurs="1" minOccurs="1" />
				<xs:element name="authors" maxOccurs="1" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="author" minOccurs="1" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="contribution" type="xs:string" />
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="contributors" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="contributor" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="contribution" type="xs:string" />
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="rules" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="rule" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="broadcasts" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:choice maxOccurs="unbounded">
							<xs:element name="tip">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="after" type="TimePeriod"/>
											<xs:attribute name="every" type="TimePeriod"/>
											<xs:attribute name="count">
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:pattern value="(\p{N}+)|oo"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="alert">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="after" type="TimePeriod"/>
											<xs:attribute name="every" type="TimePeriod"/>
											<xs:attribute name="count">
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:pattern value="(\p{N}+)|oo"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:choice>
					</xs:complexType>
				</xs:element>
				<xs:element name="teams" maxOccurs="1" minOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="team" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="color" type="xs:string" />
											<xs:attribute name="max" type="xs:integer" />
											<xs:attribute name="max-overfill" type="xs:integer" />
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="kits" maxOccurs="1" minOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="kit" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="Types">
											<xs:attribute name="name" type="xs:string" />
											<xs:attribute name="parents" type="xs:string" />
											<xs:attribute name="force" type="xs:boolean" />
											<xs:attribute name="potion-particles" type="xs:boolean" />
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="spawns" maxOccurs="1" minOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="spawn" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="Regions">
											<xs:attribute name="team" type="xs:string"/>
											<xs:attribute name="safe" type="xs:boolean"/>
											<xs:attribute name="sequential" type="xs:boolean"/>
											<xs:attribute name="kit" type="xs:string"/>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="wools" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="wool" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="block" type="xs:string"/>
									</xs:sequence>
											<xs:attribute name="team" type="xs:string"/>
											<xs:attribute name="color" type="xs:string"/>
											<xs:attribute name="craftable" type="xs:boolean"/>
											<xs:attribute name="show" type="xs:boolean"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="team" type="xs:string"/>
						<xs:attribute name="color" type="xs:string"/>
						<xs:attribute name="craftable" type="xs:boolean"/>
						<xs:attribute name="show" type="xs:boolean"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="cores" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="core" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="Regions">
											<xs:attribute name="leak" type="xs:integer"/>
											<xs:attribute name="material" type="xs:string"/>
											<xs:attribute name="team" type="xs:string"/>
											<xs:attribute name="show" type="xs:boolean"/>
											<xs:attribute name="mode-changes" type="xs:boolean"/>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="leak" type="xs:integer"/>
						<xs:attribute name="material" type="xs:string"/>
						<xs:attribute name="team" type="xs:string"/>
						<xs:attribute name="show" type="xs:boolean"/>
						<xs:attribute name="mode-changes" type="xs:boolean"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="destroyables" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="destroyable" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="Regions">
											<xs:attribute name="name" type="xs:string"/>
											<xs:attribute name="materials" type="xs:string"/>
											<xs:attribute name="owner" type="xs:string"/>
											<xs:attribute name="completion">
												<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:pattern value="([0-100])+%"/>
														</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
											<xs:attribute name="mode-changes" type="xs:boolean"/>
											<xs:attribute name="show-progress" type="xs:boolean"/>
											<xs:attribute name="repairable" type="xs:boolean"/>
											<xs:attribute name="sparks" type="xs:boolean"/>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="name" type="xs:string"/>
						<xs:attribute name="materials" type="xs:string"/>
						<xs:attribute name="owner" type="xs:string"/>
						<xs:attribute name="completion">
							<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:pattern value="([0-100])+%"/>
									</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="mode-changes" type="xs:boolean"/>
						<xs:attribute name="show-progress" type="xs:boolean"/>
						<xs:attribute name="repairable" type="xs:boolean"/>
						<xs:attribute name="sparks" type="xs:boolean"/>
					</xs:complexType>
				</xs:element>
				<xs:element name="score" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="time" type="xs:integer" minOccurs="0" maxOccurs="unbounded"/>
							<xs:element name="limit" type="xs:integer" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="blitz" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="time" minOccurs="0" maxOccurs="unbounded">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:pattern value="\p{N}(y|mo|d|h|m|s)"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="lives" type="xs:positiveInteger" minOccurs="0" maxOccurs="unbounded"/>
							<xs:element name="broadcastLives" type="xs:boolean" minOccurs="0" maxOccurs="unbounded"/>
							<xs:element name="title" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="include" type="includes" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="ghostsquadron" minOccurs="0" maxOccurs="1"/>
				<xs:element name="rage" minOccurs="0" maxOccurs="1"/>
				<xs:element name="tutorial" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="stage" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="TutorialType">
											<xs:attribute name="title" type="xs:string"/>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="king" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="hills" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="hill" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="capture" minOccurs="0" maxOccurs="unbounded" type="Regions"/>
													<xs:element name="progress" minOccurs="0" maxOccurs="unbounded" type="Regions"/>
													<xs:element name="captured" minOccurs="0" maxOccurs="unbounded" type="Regions"/>
													<xs:element name="capture-filter" minOccurs="0" maxOccurs="unbounded">
														<xs:complexType>
															<xs:attribute name="filter" type="xs:string"/>
														</xs:complexType>
													</xs:element>
													<xs:element name="player-filter" minOccurs="0" maxOccurs="unbounded">
														<xs:complexType>
															<xs:attribute name="filter" type="xs:string"/>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="name" type="xs:string"/>
												<xs:attribute name="capture-time" type="TimePeriod"/>
												<xs:attribute name="points" type="xs:integer"/>
												<xs:attribute name="points-growth" type="xs:integer"/>
												<xs:attribute name="capture-rule">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:pattern value="exclusive|majority|lead"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="scaled-time" type="xs:boolean"/>
												<xs:attribute name="show-progress" type="xs:boolean"/>
												<xs:attribute name="natural-state" type="xs:boolean"/>
												<xs:attribute name="incremental" type="xs:boolean"/>
												<xs:attribute name="permanent" type="xs:boolean"/>
												<xs:attribute name="initial-owner" type="xs:string"/>
												<xs:attribute name="show" type="xs:boolean"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="name" type="xs:string"/>
									<xs:attribute name="capture-time" type="TimePeriod"/>
									<xs:attribute name="points" type="xs:integer"/>
									<xs:attribute name="points-growth" type="xs:integer"/>
									<xs:attribute name="capture-rule">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:pattern value="exclusive|majority|lead"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="scaled-time" type="xs:boolean"/>
									<xs:attribute name="show-progress" type="xs:boolean"/>
									<xs:attribute name="natural-state" type="xs:boolean"/>
									<xs:attribute name="incremental" type="xs:boolean"/>
									<xs:attribute name="permanent" type="xs:boolean"/>
									<xs:attribute name="initial-owner" type="xs:string"/>
									<xs:attribute name="show" type="xs:boolean"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="toolrepair" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="tool" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="itemremove" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="item" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="name" type="xs:string" />
											<xs:attribute name="enchantment" type="xs:string"/>
											<xs:attribute name="lore" type="xs:string" />
											<xs:attribute name="damage" type="xs:integer" />
											<xs:attribute name="attributes" type="xs:string" />
											<xs:attribute name="color" type="xs:hexBinary" />
											<xs:attribute name="potions" type="xs:string"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="itemkeep" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="item" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="name" type="xs:string" />
											<xs:attribute name="enchantment" type="xs:string"/>
											<xs:attribute name="lore" type="xs:string" />
											<xs:attribute name="damage" type="xs:integer" />
											<xs:attribute name="attributes" type="xs:string" />
											<xs:attribute name="color" type="xs:hexBinary" />
											<xs:attribute name="potions" type="xs:string"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="disabledamage" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="damage" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="damage">
											<xs:attribute name="ally" type="xs:boolean"/>
											<xs:attribute name="self" type="xs:boolean"/>
											<xs:attribute name="enemy" type="xs:boolean"/>
											<xs:attribute name="other" type="xs:boolean"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="modifybowprojectile" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="projectile" type="entities"/>
							<xs:element name="velocityMod" type="xs:double"/>
							<xs:element name="potion" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="potions">
											<xs:attribute name="duration" type="xs:integer"/>
											<xs:attribute name="amplifier" type="xs:integer"/>
											<xs:attribute name="ambient" type="xs:boolean"/>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="tnt" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="instantignite" minOccurs="0" maxOccurs="unbounded">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:pattern value="on|off"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="blockdamage" minOccurs="0" maxOccurs="unbounded">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:pattern value="on|off"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="yield" minOccurs="0" maxOccurs="unbounded">
								<xs:simpleType>
									<xs:restriction base="xs:double">
										<xs:maxInclusive value="1.0"/>
										<xs:minInclusive value="0"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="power" minOccurs="0" maxOccurs="unbounded">
								<xs:simpleType>
									<xs:restriction base="xs:double">
										<xs:maxInclusive value="20.0"/>
										<xs:minInclusive value="0"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="fuse" minOccurs="0" maxOccurs="unbounded" type="TimePeriod"/>
							<xs:element name="dispenser-tnt-limit" minOccurs="0" maxOccurs="unbounded">
								<xs:simpleType>
									<xs:restriction base="xs:positiveInteger">
										<xs:maxInclusive value="64"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="dispenser-tnt-multiplier" minOccurs="0" maxOccurs="unbounded">
								<xs:simpleType>
									<xs:restriction base="xs:double">
										<xs:maxInclusive value="2.0"/>
										<xs:minInclusive value="0"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="filters" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="filter" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="FilterTypes">
											<xs:attribute name="name" type="xs:string"/>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="mobs" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="filter" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="MobFilterTypes">
											<xs:attribute name="name" type="xs:string"/>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="killreward" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="filter" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="FilterTypes">
											<xs:attribute name="name" type="xs:string"/>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="item" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:string">
											<xs:attribute name="amount" type="xs:integer" />
											<xs:attribute name="name" type="xs:string" />
											<xs:attribute name="enchantment" type="xs:string"/>
											<xs:attribute name="lore" type="xs:string" />
											<xs:attribute name="damage" type="xs:integer" />
											<xs:attribute name="attributes" type="xs:string" />
											<xs:attribute name="color" type="xs:hexBinary" />
											<xs:attribute name="potions" type="xs:string"/>
										</xs:extension>
								</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="gamerules" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="doFireTick" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
							<xs:element name="doTileDrops" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
							<xs:element name="doMobLoot" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
							<xs:element name="mobGriefing" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
							<xs:element name="naturalRegeneration" type="xs:boolean" minOccurs="0" maxOccurs="unbounded"/>
							<xs:element name="doDaylightCycle" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="timelock" minOccurs="0" maxOccurs="1">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="on|off"/>
